{
  "name": "@oi4/oi4-oec-pki-demo",
  "version": "1.0.0-preview.2",
  "description": "Simple PKI for demo usage. THIS IS NOT SECURE! DO NOT USE IT FOR SOMETHING ELSE THAN TESTING!",
  "author": "Open Industry 4.0 Alliance - Community",
  "license": "MIT",
  "private": false,
  "keywords": [
    "oi4",
    "pki",
    "demo"
  ],
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:OI4/oi4-oec-demo.git"
  },
  "scripts": {
    "build": "yarn run clean && echo 'building logger service demo project' && tsc -b tsconfig.json",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "deploy": "echo \"Deploy not specified\" && exit 0",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "package": "echo \"Package not specified\" && exit 0",
    "test": "jest ",
    "copy:dependencies": "source ./scripts/setenv.sh && bash ./scripts/copy_dependencies.sh &&",
    "docker:prepare": "docker buildx create --name oi4-builder --driver docker-container --use --platform linux/amd64,linux/arm64,linux/arm/v7,linux/arm/v6,linux/386",
    "docker:build:local": "docker build -t oi4/oi4-oec-pv-logger-demo:${npm_package_version} -t oi4/oi4-oec-pv-logger-demo:latest --build-arg PACKAGES_AUTH_TOKEN=${PACKAGES_AUTH_TOKEN} .",
    "docker:build:snapshot": "docker build -t oi4/oi4-oec-pv-logger-demo:${npm_package_version} -t oi4/oi4-oec-pv-logger-demo:latest --build-arg PACKAGES_AUTH_TOKEN=${PACKAGES_AUTH_TOKEN} --build-arg BUILD_ENV=snapshot .",
    "testLocal": "chmod ug+x ./scripts/test-local.sh && ./scripts/test-local.sh"
  }
}
